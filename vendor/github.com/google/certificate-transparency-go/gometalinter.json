{
  "Deadline": "60s",
  "Linters": {
    "license": "./scripts/check_license.sh:PATH:LINE:MESSAGE",
    "forked": "./scripts/check_forked.sh:PATH:LINE:MESSAGE",
    "unforked": "./scripts/check_unforked.sh:PATH:LINE:MESSAGE"
  },
  "Enable": [
    "forked",
    "gocyclo",
    "gofmt",
    "goimports",
    "golint",
    "license",
    "misspell",
